%tr{:class => 'edit js_edit'}
	%td.line_ck= f.check_box :is_enabled
	%td{:class => 'line_num editable'}= f.text_field :number
	%td{:class => 'line_name editable'}= f.text_field :name
	%td{:class => 'line_qty editable'}= f.text_field :quantity
	%td{:class => 'line_u_price editable'}= f.text_field :unit_price, :value => number_with_precision(f.object.unit_price, :precision => 2)
	%td.line_t_price= number_with_precision(f.object.total_price, :precision => 2, :delimiter => ',')
	%td.line_links= link_to_remove_fields "Delete", f